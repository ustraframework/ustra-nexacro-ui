var button = {
	
	AUTH_BUTTON_INITIALIZED_EVENT_NAME: 'AUTH_BUTTON_INITIALIZED',
	
	// 초기화
	_init: function(button) {
		$ustra.events.fireEvent($ustra.component.button.AUTH_BUTTON_INITIALIZED_EVENT_NAME, button);
	}
}

$ustra.module('component', { button: button });
]]></Script>
